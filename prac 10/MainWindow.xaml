<Window x:Class="prac_10.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:prac_10"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <ListBox x:Name="listBox" HorizontalAlignment="Left" Height="163" Margin="111,51,0,0" ItemsSource="{Binding Teamlist}" VerticalAlignment="Top" Width="260" SelectionChanged="listBox_SelectionChanged" />
        <TextBox x:Name="textBlock" FontWeight="ExtraBold" FontSize="30"  HorizontalAlignment="Center" Background="Beige" TextWrapping="Wrap" Text="{Binding Winner}" VerticalAlignment="bottom"/>

    </Grid>
</Window>
